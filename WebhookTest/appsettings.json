{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://*:5052"
      }
    }
  },
  
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "RedisSettings": {
    "AbortOnConnectFail": false,
    "ConnectRetry": 5,
    "ReconnectRetryDelayMs": 5000
  }
}
